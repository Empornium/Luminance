<?xml version="1.0" encoding="UTF-8"?>

<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/" xmlns:moz="http://www.mozilla.org/2006/browser/search/">
    <ShortName>{{ shortName }}</ShortName>
    <Description>{{ description }}</Description>
    <Developer></Developer>
    <Image width="16" height="16" type="image/x-icon">{{ urlBase }}/favicon.ico</Image>
    <Url type="text/html" method="get" template="{{ templatedSearchUrl }}"></Url>
    <moz:SearchForm>{{ mozSearchFormUrl }}</moz:SearchForm>
    <Url type="application/opensearchdescription+xml" rel="self" template="{{ urlBase }}/opensearch?type={{ searchType }}" />
    <Language>en-us</Language>
    <OutputEncoding>UTF-8</OutputEncoding>
    <InputEncoding>UTF-8</InputEncoding>
</OpenSearchDescription>
